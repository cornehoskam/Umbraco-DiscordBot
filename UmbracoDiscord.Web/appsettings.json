{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "../UmbracoDiscord.Models/ModelsBuilder/",
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "UmbracoDiscord.ModelsBuilder"
      },
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Global": {
        "Id": "5cbd8032-5a61-411a-8734-25efd1a05463"
      }
    }
  }
}